class Point2D {
  final double x;
  final double y;
  Point2D(double x, double y){
    this.x = x;
    this.y = y;
  }
}

class Rect {
  final float x;
  final float y;
  final float width;
  final float height;
  Rect(float x, float y, float width, float height){
    this.x = x;
    this.y = y;
    this.width = width;
    this.height = height;
  }
}
